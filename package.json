{
  "name": "personal-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://zizizheng.github.io/personal-web",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-router-hash-link": "^1.0.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass src/assets/sass -o src/assets/css",
    "watch-css": "npm run build-css && node-sass src/assets/sass -o src/assets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
